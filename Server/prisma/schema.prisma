generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Employee {
  id            Int      @id @default(autoincrement())
  name          String
  employeeId    String   @unique
  email         String   @unique
  phoneNumber   String
  department    String
  dateOfJoining DateTime
  role          String
}
